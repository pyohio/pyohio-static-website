      PyOhio 2023 info

    </Fragment>
  </Hero>
  <CallToAction
  callToAction={{
    text: 'Subscribe',
    href: 'https://www.example.com/',
    icon: 'tabler:mail',
  }}
>
  <Fragment slot="title">
Join our mailing list  </Fragment>
  <Fragment slot="subtitle">
Mailing list info
  </Fragment>
</CallToAction> 
</Layout>

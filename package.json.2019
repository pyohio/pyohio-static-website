  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "npm run develop",
    "clean": "rimraf .cache public",
    "build": "npm run clean && gatsby build --prefix-paths",
    "build-prod": "npm run build && mv -v ./public ./_public && mkdir -p ./public && mv -v ./_public ./public/2019 && cp -r ./archive/20* ./public && cp -rv ./netlify-static/* ./public/",
    "develop": "npm run clean && gatsby develop --prefix-paths --host 0.0.0.0",
    "serve": "gatsby serve --prefix-paths --host 0.0.0.0",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2"
  }
}
